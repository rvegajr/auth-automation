{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AdfsAuth": {
    "OutputFile": "tokens.json",
    "Headless": false,
    "SlowMo": 100,
    "Timeout": 600000,
    "CredentialsJsonFile": "../.parameters",
    "Screenshots": {
      "Enabled": true,
      "Directory": "screenshots",
      "Prefix": "auth_"
    },
    "TokenCacheFile": "adfs_token_cache.json",
    "AutoInstallDependencies": true
  }
}
